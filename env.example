WEBSERVER_DOC_ROOT=/var/www/html
NGINX_PREFIX=/etc/nginx
LOCAL_TIMEZONE=local_timezone
DOMAIN_NAME=example.com
DIRECTORY_PATH=directory_path

LETSENCRYPT_EMAIL=email@domain.com
LETSENCRYPT_CONF_PREFIX=/etc/letsencrypt
SSL_SNIPPET=ssl_snippet

PHP_IMAGE_NAME=php:8.4-fpm
PHP_INI_DIR_PREFIX=/usr/local/etc

DB_USER=db_username
DB_PASSWORD=db_password
DB_NAME=db_name
DB_TABLE_PREFIX=db_table_prefix

MYSQL_CONF_PREFIX=/etc/mysql/conf.d
MYSQL_ROOT_PASSWORD=mysql_root_password
MYSQL_ROOT_HOST=%
DATABASE_IMAGE_NAME=which_db
DATABASE_CONT_NAME=which_db
DATABASE_VERSION=latest
DATABASE_PACKAGE_MANAGER=db_package_manager
DATABASE_ADMIN_COMMANDLINE=db_admin_commandline
DATABASE_CONNECT_EXTENSION=db_connect_extension

PMA_CONTROLUSER=pma_username
PMA_CONTROLPASS=pma_password
PMA_CONF_FOLDER=/etc/phpmyadmin 
PMA_HTPASSWD_USERNAME=db_username
PMA_HTPASSWD_PASSWORD=db_password
PMA_UPLOAD_LIMIT=8M
PMA_MEMORY_LIMIT=-1
APACHE_CONFDIR_PREFIX=/etc/apache2

REDIS_CONF_PREFIX=/usr/local/etc
